{
  "compilerOptions": {
    "target": "es6",
    "module": "ES2015",
    "declaration": true,
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true
  }
}
