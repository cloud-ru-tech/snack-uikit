{
  "name": "uikit-product",
  "description": "Sbercloud Product Uikit",
  "homepage": "https://git.sbercloud.tech/sbercloud-ui/uikit-tokens-demo/-/tree/master/",
  "repository": {
    "type": "git",
    "url": "https://git.sbercloud.tech/sbercloud-ui/uikit-tokens-demo.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "build-storybook",
    "build:packages": "npm run clean:all && npm run deps:packages && lerna run compile && ts-node scripts/compile",
    "build:tokens": "ts-node tokens/build-tokens.ts",
    "build:style-dictionary:config": "style-dictionary build --config tokens/sd.config.js",
    "build:style-dictionary:custom-script": "ts-node tokens/styled-dictionary.ts",
    "build:styles:all": "rm -rf tokens/build && npm run build:tokens && npm run build:style-dictionary:custom-script",
    "changelog": "ts-node scripts/changelog",
    "clean:all": "npm run clean:dist && npm run clean:modules",
    "clean:dist": "rimraf \"./packages/**/dist/\"",
    "clean:modules": "rimraf \"./packages/**/node_modules/\"",
    "deps:all": "npm run deps:global && npm run deps:packages",
    "deps:global": "npm i",
    "deps:packages": "npm i --workspaces",
    "deps:reinstall": "npm run clean:all && rm -rf node_modules package-lock.json && npm cache clean -f && npm run deps:all",
    "storybook:all": "start-storybook -p 6006",
    "storybook:ci": "cross-env TEST_SERVER=true start-storybook -p 6006 --ci --quiet --loglevel warn",
    "storybook:partial": "ts-node scripts/start-storybook-partially",
    "test:coverage-all": "ts-node scripts/test-coverage-all-packages",
    "test:coverage-unstable": "ts-node scripts/test-coverage-unstable-packages",
    "test:coverage-stable": "ts-node scripts/test-coverage-stable-packages",
    "test:testcafe": "ts-node scripts/run-tests && ts-node scripts/normalize-testcafe-xml-report",
    "test:testcafe:local": "testcafe chrome --config-file testcafe.config.js --live --debug-on-fail --concurrency 1",
    "test:ci": "start-server-and-test storybook:ci http://localhost:6006 test:testcafe"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-modules-commonjs": "7.19.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@geometricpanda/storybook-addon-badges": "0.2.2",
    "@linaria/cli": "3.0.0-beta.13",
    "@sbercloud/ft-all-linters-pack": "2.1.0",
    "@sbercloud/ft-config-linaria": "1.1.0",
    "@sbercloud/ft-conventional-changelog": "1.2.1",
    "@sbercloud/ft-storybook-readme-addon": "1.1.0",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-storysource": "6.5.13",
    "@storybook/addons": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/cli": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@types/babel__core": "7.1.20",
    "@types/express": "4.17.14",
    "@types/inquirer": "8.2.5",
    "@types/mkdirp": "1.0.2",
    "@types/node": "16.18.3",
    "@types/normalize-path": "3.0.0",
    "@types/react": "17.0.52",
    "@types/react-dom": "17.0.18",
    "@types/react-syntax-highlighter": "15.5.5",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "babel-loader": "9.1.0",
    "babel-plugin-istanbul": "6.1.1",
    "colors": "1.4.0",
    "cross-env": "7.0.3",
    "depcheck": "1.4.3",
    "dependency-graph": "0.11.0",
    "eslint-plugin-testcafe-community": "2.0.0",
    "glob": "8.0.3",
    "inquirer": "8.2.5",
    "lerna": "5.6.2",
    "minimist": "1.2.7",
    "mkdirp": "1.0.4",
    "normalize-path": "3.0.0",
    "nyc": "15.1.0",
    "react": "17.0.2",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.38.0",
