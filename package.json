    "token-transformer": "0.0.29",
    "ts-config-single-spa": "3.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.7.4",
    "webpack": "5.75.0"
  },
