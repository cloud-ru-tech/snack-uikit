{
  "name": "snack-ui",
  "description": "Cloud Product Uikit",
  "homepage": "https://git.sbercloud.tech/sbercloud-ui/tokens-design-system/snack-uikit/-/tree/master/",
  "repository": {
    "type": "git",
    "url": "https://git.sbercloud.tech/sbercloud-ui/tokens-design-system/snack-uikit.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "storybook build -c storybook",
    "build:packages": "npm run clean:all && npm run deps:packages && lerna run compile && npm run build:ts && npm run build:css",
    "build:css": "ts-node scripts/compileCSS",
    "build:ts": "ttsc -b ./packages/tsconfig.json",
    "changelog": "ts-node scripts/changelog",
    "clean:all": "npm run clean:dist && npm run clean:modules && npm run clean:buildinfo",
    "clean:buildinfo": "rimraf ./packages/**/*.tsbuildinfo ./packages/npm-package.tsconfig.tsbuildinfo",
    "clean:dist": "rimraf ./packages/*/dist",
    "clean:modules": "rimraf ./packages/*/node_modules",
    "deps:all": "npm run deps:global && npm run deps:packages",
    "deps:global": "npm i",
    "deps:packages": "npm i --workspaces",
    "deps:reinstall": "npm run clean:all && rm -rf node_modules package-lock.json && npm cache clean -f && npm run deps:all",
    "storybook:all": "storybook dev -c storybook -p 6006",
    "storybook:ci": "cross-env TEST_SERVER=true storybook dev -c storybook -p 6006 --ci --quiet --loglevel warn",
    "storybook:partial": "ts-node scripts/start-storybook-partially",
    "test:coverage": "ts-node scripts/testcafe-coverage-packages",
    "test:testcafe": "ts-node scripts/run-tests && ts-node scripts/normalize-testcafe-xml-report",
    "test:testcafe:local": "cross-env TEST_LOCAL=true testcafe chrome --config-file testcafe.config.js --live",
    "test:ci": "start-server-and-test storybook:ci http://localhost:6006 test:testcafe",
    "test:unit": "vitest run",
    "test:unit:local": "vitest"
  },
  "devDependencies": {
    "@geometricpanda/storybook-addon-badges": "2.0.0",
    "@sbercloud/figma-tokens": "1.2.2",
    "@sbercloud/figma-tokens-cloud-platform": "1.2.2",
    "@sbercloud/figma-tokens-mlspace": "1.2.2",
    "@sbercloud/ft-all-linters-pack": "2.2.8",
    "@sbercloud/ft-config-babel": "2.1.1",
    "@sbercloud/ft-config-vitest": "1.1.1",
    "@sbercloud/ft-conventional-changelog": "1.2.3",
    "@sbercloud/ft-copy-to-clipboard": "2.1.0",
    "@sbercloud/ft-storybook-brand-addon": "1.2.0",
    "@sbercloud/ft-storybook-readme-addon": "3.0.1",
    "@storybook/addon-actions": "7.0.7",
    "@storybook/addon-essentials": "7.0.7",
    "@storybook/addon-links": "7.0.7",
    "@storybook/addon-storysource": "7.0.7",
    "@storybook/cli": "7.0.7",
    "@storybook/manager-api": "7.0.7",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "7.0.7",
    "@storybook/react-webpack5": "7.0.7",
    "@storybook/theming": "7.0.7",
    "@types/express": "4.17.17",
    "@types/inquirer": "8.2.6",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.16.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/react-syntax-highlighter": "15.5.6",
    "@types/shelljs": "0.8.12",
    "autoprefixer": "10.4.14",
    "babel-plugin-istanbul": "6.1.1",
    "colors": "1.4.0",
    "cross-env": "7.0.3",
    "cross-var": "1.1.0",
    "depcheck": "1.4.3",
    "eslint-plugin-testcafe-community": "2.0.0",
    "fast-xml-parser": "4.2.2",
    "glob": "10.2.2",
    "inquirer": "8.2.5",
    "lerna": "6.6.1",
    "lodash.debounce": "4.0.8",
    "minimist": "1.2.8",
    "nyc": "15.1.0",
    "postcss": "8.4.23",
    "postcss-discard-comments": "6.0.0",
    "react": "18.2.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.7",
    "react-syntax-highlighter": "15.5.0",
    "rimraf": "5.0.0",
    "sass": "1.62.1",
    "shelljs": "0.8.5",
    "start-server-and-test": "2.0.0",
    "storybook": "7.0.7",
    "storybook-addon-designs": "7.0.0-beta.2",
    "storybook-dark-mode": "3.0.0",
    "stream-browserify": "3.0.0",
    "stylelint": "15.6.0",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended-scss": "11.0.0",
    "stylelint-scss": "4.6.0",
    "testcafe": "2.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "ttypescript": "1.5.15",
    "typescript": "4.9.5",
    "typescript-plugin-css-modules": "5.0.1",
    "webpack": "5.81.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "workspaces": [
    "./packages/*"
  ]
}
