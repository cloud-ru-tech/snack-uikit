    "svg-inline-loader": "0.8.2",
    "testcafe": "3.7.1",
    "ts-node": "10.9.2",
    "ts-patch": "3.2.1",
    "typescript": "5.5.2",
    "typescript-plugin-css-modules": "5.1.0",
    "vite": "6.3.5",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "yaml": "2.8.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "stylus": "0.0.1-security"
    }
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "pnpm": "^10.10.0"
  }
}