    "copy-to-clipboard": "3.3.3",
    "cross-env": "7.0.3",
    "cross-var": "1.1.0",
    "depcheck": "1.4.7",
    "dotenv": "16.4.5",
    "express": "5.1.0",
    "glob": "11.0.0",
