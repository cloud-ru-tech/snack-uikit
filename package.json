{
  "name": "uikit-template",
  "homepage": "https://git.sbercloud.tech/sbercloud-ui/uikit-template/-/tree/master/",
  "repository": {
    "type": "git",
    "url": "https://git.sbercloud.tech/sbercloud-ui/uikit-template.git"
  },
  "description": "Sbercloud UI-Kit template",
  "license": "UNLICENSED",
  "scripts": {
    "partial-storybook": "ts-node scripts/start-storybook-partially.ts",
    "storybook": "start-storybook -p 6006",
    "storybook:ci": "start-storybook -p 6006 --ci",
    "build:storybook": "build-storybook",
    "add-package": "ts-node scripts/npm-init",
    "clean": "npm run clean:dist && npm run clean:modules",
    "clean:dist": "rimraf \"./packages/**/dist/\"",
    "clean:modules": "rimraf \"./packages/**/node_modules/\"",
    "changelog": "ts-node scripts/changelog.ts",
    "compile": "lerna run compile && ts-node scripts/compile.ts",
    "build": "npm run clean && lerna bootstrap --hoist && npm run compile",
    "all:cleaninstall": "node scripts/remove-modules.js && npm cache clean -f && npm i --legacy-peer-deps && npm run build",
    "prepare": "node_modules/.bin/husky install",
    "test": "cypress run --headless --quiet",
    "ci": "start-server-and-test storybook:ci http://localhost:6006 test"
  },
