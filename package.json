{
  "name": "@snack-ui",
  "description": "Cloud Product Uikit",
  "homepage": "https://git.sbercloud.tech/sbercloud-ui/uikit-tokens-demo/-/tree/master/",
  "repository": {
    "type": "git",
    "url": "https://git.sbercloud.tech/sbercloud-ui/uikit-tokens-demo.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "build-storybook -c storybook",
    "build:packages": "npm run clean:all && npm run deps:packages && npm run build:ts && npm run build:css",
    "build:css": "ts-node scripts/compileCSS",
    "build:ts": "ttsc -b ./packages/tsconfig.json",
    "changelog": "ts-node scripts/changelog",
    "clean:all": "npm run clean:dist && npm run clean:modules && npm run clean:buildinfo",
    "clean:buildinfo": "rimraf ./packages/**/*.tsbuildinfo ./packages/npm-package.tsconfig.tsbuildinfo",
    "clean:dist": "rimraf ./packages/*/dist",
    "clean:modules": "rimraf ./packages/*/node_modules",
    "deps:all": "npm run deps:global && npm run deps:packages",
    "deps:global": "npm i",
    "deps:packages": "npm i --workspaces",
    "deps:reinstall": "npm run clean:all && rm -rf node_modules package-lock.json && npm cache clean -f && npm run deps:all",
    "storybook:all": "start-storybook -c storybook -p 6006",
    "storybook:ci": "cross-env TEST_SERVER=true start-storybook -c storybook -p 6006 --ci --quiet --loglevel warn",
    "storybook:partial": "ts-node scripts/start-storybook-partially",
    "test:coverage": "ts-node scripts/testcafe-coverage-packages",
    "test:testcafe": "ts-node scripts/run-tests && ts-node scripts/normalize-testcafe-xml-report",
    "test:testcafe:local": "testcafe chrome --config-file testcafe.config.js --live --debug-on-fail --concurrency 1",
    "test:ci": "start-server-and-test storybook:ci http://localhost:6006 test:testcafe"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.20.7",
    "@babel/plugin-transform-modules-commonjs": "7.20.11",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@geometricpanda/storybook-addon-badges": "0.2.2",
    "@sbercloud/figma-tokens": "0.6.0",
    "@sbercloud/ft-all-linters-pack": "2.1.4",
    "@sbercloud/ft-conventional-changelog": "1.2.3",
    "@sbercloud/ft-storybook-readme-addon": "2.0.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/cli": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.16",
    "@types/babel__core": "7.20.0",
    "@types/express": "4.17.17",
    "@types/inquirer": "8.2.6",
    "@types/node": "16.18.12",
    "@types/normalize-path": "3.0.0",
    "@types/react": "17.0.53",
    "@types/react-dom": "17.0.19",
    "@types/shelljs": "0.8.11",
    "babel-loader": "9.1.2",
    "babel-plugin-istanbul": "6.1.1",
    "colors": "1.4.0",
    "cross-env": "7.0.3",
    "depcheck": "1.4.3",
    "eslint-plugin-testcafe-community": "2.0.0",
    "glob": "8.1.0",
    "inquirer": "8.2.5",
    "lerna": "5.6.2",
    "minimist": "1.2.8",
    "normalize-path": "3.0.0",
    "nyc": "15.1.0",
    "postcss": "8.4.21",
    "postcss-discard-comments": "5.1.2",
    "react": "17.0.2",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "17.0.2",
    "rimraf": "4.1.2",
    "sass": "1.58.2",
    "shelljs": "0.8.5",
    "start-server-and-test": "1.15.4",
    "storybook": "6.5.16",
    "storybook-addon-designs": "6.3.1",
    "stream-browserify": "3.0.0",
    "stylelint": "15.1.0",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended-scss": "9.0.1",
    "stylelint-scss": "4.4.0",
    "testcafe": "2.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "ttypescript": "1.5.15",
    "typescript": "4.9.5",
    "typescript-plugin-css-modules": "4.1.1",
    "webpack": "5.75.0"
  },
  "workspaces": [
    "./packages/*"
  ]
}
