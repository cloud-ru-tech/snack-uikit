    "postcss-discard-comments": "7.0.3",
    "react": "18.2.0",
    "react-docgen": "7.1.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "18.2.0",
    "react-markdown": "7.1.1",
    "react-syntax-highlighter": "15.6.1",
    "remark-gfm": "3.0.1",
    "rimraf": "6.0.1",
    "sass": "1.80.4",
