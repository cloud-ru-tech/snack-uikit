{
  "name": "snack-uikit",
  "description": "Cloud Product Uikit",
  "homepage": "https://cloud-ru-tech.github.io/snack-uikit/",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-ru-tech/snack-uikit/tree/master"
  },
  "license": "Apache-2.0",
  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "npm run install:themes && cross-env TEST_SERVER=true storybook build -c storybook",
    "build:packages:esm": "tspc -b ./packages/tsconfig.esm.json",
    "build:packages:cjs": "tspc -b ./packages/tsconfig.cjs.json",
    "build:packages": "npm run clean:all && npm run deps:packages && lerna run compile && npm run build:ts && npm run build:css",
    "build:css": "ts-node scripts/compileCSS && ts-node scripts/compileJsCssModules",
    "docgen": "ts-node scripts/docgen/docgenForAllPackages",
    "build:ts": "npm run build:packages:esm && npm run build:packages:cjs",
    "changelog": "ts-node scripts/changelog",
    "clean:all": "npm run clean:empty && npm run clean:dist && npm run clean:modules && npm run clean:buildinfo",
    "clean:buildinfo": "rimraf --glob ./packages/**/*.tsbuildinfo ./packages/npm-package.tsconfig.tsbuildinfo",
    "clean:dist": "rimraf --glob ./packages/*/dist",
    "clean:modules": "rimraf --glob ./packages/*/node_modules",
    "clean:empty": "ts-node scripts/removeEmptyPackages.ts",
    "deps:all": "npm run deps:global && npm run deps:packages",
    "deps:global": "npm i",
    "deps:packages": "npm i --workspaces",
    "deps:reinstall": "npm run clean:all && rm -rf node_modules package-lock.json && npm cache clean -f && npm run deps:all",
    "storybook:all": "npm run install:themes && storybook dev -c storybook -p 6006 --https --ssl-cert ../cert.crt --ssl-key ../cert.key",
    "storybook:partial": "npm run install:themes && ts-node scripts/start-storybook-partially",
    "test:coverage": "ts-node scripts/testcafe-coverage-packages",
    "test:ci": "ts-node scripts/run-tests",
    "test:testcafe": "testcafe chrome --config-file testcafe.config.js --live",
    "test:testcafe:local": "cross-env TEST_LOCAL=true npm run test:testcafe",
    "test:testcafe:branch": "cross-env TEST_ON_BRANCH=true npm run test:testcafe",
    "test:unit": "vitest run",
    "test:unit:local": "vitest",
    "install:themes": "ts-node scripts/install-additional-themes"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@cloud-ru/eslint-config": "2.1.0",
    "@cloud-ru/eslint-plugin-ssr-safe-react": "0.3.0",
    "@cloud-ru/ft-config-babel": "1.1.2",
    "@cloud-ru/ft-config-commit-message": "1.1.1",
    "@cloud-ru/ft-config-husky": "1.1.2",
    "@cloud-ru/ft-config-lint-staged": "1.1.2",
    "@cloud-ru/ft-config-prettier": "1.1.2",
    "@cloud-ru/ft-config-stylelint": "2.0.0",
    "@cloud-ru/ft-config-tsconfig": "1.1.1",
    "@cloud-ru/ft-config-vitest": "1.2.1",
    "@cloud-ru/ft-conventional-changelog-old": "npm:@cloud-ru/ft-conventional-changelog@1.1.0",
    "@cloud-ru/ft-license-validator": "1.1.1",
    "@cloud-ru/ft-storybook-brand-addon": "2.0.5",
    "@cloud-ru/ft-storybook-deps-graph-addon": "2.0.3",
    "@cloud-ru/ft-storybook-readme-addon": "2.0.3",
    "@geometricpanda/storybook-addon-badges": "2.0.5",
    "@snack-uikit/figma-tokens": "17.7.0",
    "@storybook/addon-a11y": "8.3.6",
    "@storybook/addon-actions": "8.3.6",
    "@storybook/addon-designs": "8.0.3",
    "@storybook/addon-essentials": "8.3.6",
    "@storybook/addon-links": "8.3.6",
    "@storybook/addon-storysource": "8.3.6",
    "@storybook/addon-webpack5-compiler-babel": "3.0.3",
    "@storybook/cli": "8.3.6",
    "@storybook/manager-api": "8.3.6",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/preview-api": "8.3.6",
    "@storybook/react": "8.3.6",
    "@storybook/react-webpack5": "8.3.6",
    "@storybook/theming": "8.3.6",
    "@types/express": "4.17.21",
    "@types/inquirer": "8.2.10",
    "@types/istanbul-lib-coverage": "2.0.6",
    "@types/lodash.debounce": "4.0.9",
    "@types/node": "22.7.9",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/shelljs": "0.8.15",
    "autoprefixer": "10.4.20",
    "babel-plugin-istanbul": "7.0.0",
    "babel-plugin-react-css-modules": "5.2.6",
    "colors": "1.4.0",
    "copy-to-clipboard": "3.3.3",
    "cross-env": "7.0.3",
    "cross-var": "1.1.0",
    "depcheck": "1.4.7",
    "dotenv": "16.4.5",
    "glob": "11.0.0",
    "inquirer": "8.2.6",
    "js-file-download": "0.4.12",
    "jsdom": "25.0.1",
    "lerna": "8.1.8",
    "lodash.debounce": "4.0.8",
    "minimist": "1.2.8",
    "monaco-editor-webpack-plugin": "7.1.0",
    "monaco-yaml": "5.2.3",
    "nyc": "17.1.0",
    "postcss": "8.4.47",
    "postcss-discard-comments": "7.0.3",
    "react": "18.2.0",
    "react-docgen": "7.1.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "18.2.0",
    "react-markdown": "9.0.1",
    "react-syntax-highlighter": "15.6.1",
    "remark-gfm": "4.0.0",
    "rimraf": "6.0.1",
    "sass": "1.80.4",
    "shelljs": "0.8.5",
    "storybook": "8.3.6",
    "storybook-dark-mode": "4.0.2",
    "stream-browserify": "3.0.0",
    "stylelint-config-clean-order": "6.1.0",
    "svg-inline-loader": "0.8.2",
    "testcafe": "3.7.1",
    "ts-node": "10.9.2",
    "ts-patch": "3.2.1",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.5.2",
    "typescript-plugin-css-modules": "5.1.0",
    "webpack": "5.95.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
