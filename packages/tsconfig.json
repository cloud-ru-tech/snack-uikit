{
  "files": [],
  "references": [{ "path": "./button" }],
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2017", "dom", "dom.iterable", "esnext"],
    "module": "ES2015",
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "composite": true,
    "plugins": [
      { "transform": "../../scripts/scss-extension-transformer.ts" },
      {
        "name": "typescript-plugin-css-modules",
        "options": {
          "customRenderer": "../../scripts/compile/sass-custom-renderer.js"
        }
      }
    ]
  }
}
